@article{moretti_amphoteric_2014,
	title = {The amphoteric behavior of water in silicate melts from the point of view of their ionic-polymeric constitution},
	volume = {367},
	issn = {0009-2541},
	url = {http://www.sciencedirect.com/science/article/pii/S0009254113005895},
	doi = {10.1016/j.chemgeo.2013.12.012},
	abstract = {Dissociation of water into protons and hydroxyl ions is a fundamental feature of aqueous solutions. Although it exerts a profound influence on properties of magmas, this autoprotolysis reaction has been hitherto neglected for water dissolved in silicate melts. As made here with an acid–base model, in fact one has to deal with molecular water (H2Omol) and two kinds of hydroxyl groups, bonded or not to network-forming cations (OH and OH−, respectively) in hydrous silicate melts. By mixing cations and anions on distinct sublattices and quantifying the disproportionation of water dissolved in silicate melts into its ionic products, H+ and OH−, we reconcile spectroscopic determinations of water speciation, and highlight the main compositional features involving chemical exchanges between H2Omol, TOH and MOH groups (T and M being a former and a modifier, respectively). In particular, water addition to depolymerized systems, such as basalts, determine a relative predominance of OH− with respect to silicic systems, such that the increase of water concentration tends to immediately limit depolymerization rather than enhance it. This opens new perspectives to the understanding of the chemical control of hydrous magmas and their physical properties, as well as the attainment of saturation in hydrous minerals such as amphiboles or micas, particularly in depolymerized melts.},
	urldate = {2016-11-04},
	journal = {Chemical Geology},
	author = {Moretti, Roberto and Le Losq, Charles and Neuville, Daniel R.},
	month = feb,
	year = {2014},
	keywords = {Polymerization, Water speciation, Autoionization, Amphoteric behavior, Acid–base properties},
	pages = {23--33},
	file = {ScienceDirect Full Text PDF:/Users/charleslelosq/Sync_me/Articles/zotero/storage/WWECG468/Moretti et al. - 2014 - The amphoteric behavior of water in silicate melts.pdf:application/pdf;ScienceDirect Snapshot:/Users/charleslelosq/Sync_me/Articles/zotero/storage/ZAWSKI8I/S0009254113005895.html:text/html},
}

